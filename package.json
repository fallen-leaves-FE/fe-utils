{
  "name": "@fallen_leaves/fe-utils",
  "version": "2.0.0-beta.1",
  "description": "前端工具函数库",
  "main": "dist/fe-utils.umd.js",
  "module": "dist/fe-utils.esm.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/fe-utils.umd.js",
      "import": "./dist/fe-utils.esm.js"
    },
    "./debounce": "./dist/modules/debounce.js",
    "./deepClone": "./dist/modules/deepClone.js",
    "./deleteUrlParams": "./dist/modules/deleteUrlParams.js",
    "./digitUppercase": "./dist/modules/digitUppercase.js",
    "./fixedThousandsNumber": "./dist/modules/fixedThousandsNumber.js",
    "./formatFileSize": "./dist/modules/formatFileSize.js",
    "./formatFixedDate": "./dist/modules/formatFixedDate.js",
    "./formatPassTime": "./dist/modules/formatPassTime.js",
    "./getCookie": "./dist/modules/getCookie.js",
    "./getExplore": "./dist/modules/getExplore.js",
    "./getOS": "./dist/modules/getOS.js",
    "./getUrlParams": "./dist/modules/getUrlParams.js",
    "./hexToRPG": "./dist/modules/hexToRPG.js",
    "./isEmpty": "./dist/modules/isEmpty.js",
    "./parseQuery": "./dist/modules/parseQuery.js",
    "./regexp": "./dist/modules/regexp.js",
    "./removeCookie": "./dist/modules/removeCookie.js",
    "./RGBToHex": "./dist/modules/RGBToHex.js",
    "./setCookie": "./dist/modules/setCookie.js",
    "./stringifyQuery": "./dist/modules/stringifyQuery.js",
    "./throttle": "./dist/modules/throttle.js",
    "./urlToLink": "./dist/modules/urlToLink.js",
    "./useFullscreen": "./dist/modules/useFullscreen.js",
    "./versionCompare": "./dist/modules/versionCompare.js"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "scripts": {
    "build": "npm run clean:dist && npm run lint && rollup -c rollup.config.js",
    "test": "npm run clean:test && jest",
    "clean:dist": "rimraf ./dist",
    "clean:test": "rimraf ./coverage",
    "lint": "eslint --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ignore-path .eslintignore ."
  },
  "keywords": [
    "utils",
    "fe-utils",
    "工具函数",
    "wecoder",
    "rollup",
    "esbuild"
  ],
  "author": {
    "name": "fallen_leaves",
    "url": "https://gitee.com/we_coder"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/we_coder/fe-utils"
  },
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^28.1.3",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "esbuild": "^0.15.6",
    "eslint": "^8.23.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-transform-stub": "^2.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.10.1",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-replace": "^2.2.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.8.2"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
